	<util:map id="addShoppingCartProduct" key-type="java.lang.String" value-type="java.lang.Object" map-class="java.util.HashMap" scope="prototype">
		<entry key="products">
			<!-- 
				Each product is defined as a hashmap. 
				The values in the hashmap are pulled by the test in order to create the product entities. 
				Each hashmap also contains the amount of items which will be placed in the shopping cart.
				This value should always be less than or equal to the number of items in stock.
			-->
			<list value-type="java.util.HashMap">
				<util:map key-type="java.lang.String" value-type="java.lang.Object" map-class="java.util.HashMap" scope="prototype">
					<entry key="type" value="simple" />
					<entry key="set" value="#{4}" />
					<entry key="sku" value="1111111C" />
					<entry key="attributesRef">
						<bean class="com.magento.api.CatalogProductCreateEntity" >
